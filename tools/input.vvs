section text
.start
    uload r2 3
    uload r5 2

    uload r3 1
    uload r4 5
    label exping
        ucmp r3 r4
        jg @exit1

        upow r2 r5
        #ncall 1 r2

        uinc r3
        jmp @exping

    label exit1
        uload r3 1

    label sqrtloop
          ucmp r3 r4
          jg @fin

          usqrt r2 r2
          #ncall 1 r2

          uinc r3
          jmp @sqrtloop


    label fin
    halt


section data
    n int 1
